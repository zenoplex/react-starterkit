{
  "private": true,
  "engines": {
    "node": ">=6"
  },
  "name": "react-starterkit",
  "version": "0.0.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf dist",
    "build:prod": "NODE_ENV=production webpack --config webpack.production.config.babel.js",
    "build": "npm run clean && npm run build:prod",
    "start": "babel-node server/index.js",
    "lint": "eslint src",
    "mocha": "NODE_ENV=test mocha --compilers js:espower-babel/guess --require test/helper.js test/**/*.js",
    "test": "npm run lint && npm run mocha",
    "test:watch": "npm run mocha -- -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zenoplex/react-starterkit.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform",
    "enzyme",
    "mocha"
  ],
  "author": "zenoplex (http://github.com/zenoplex)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zenoplex/react-starterkit/issues"
  },
  "homepage": "https://github.com/zenoplex/react-starterkit",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "enzyme": "^2.7.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.13.0",
    "espower-babel": "^4.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "flow-bin": "^0.42.0",
    "html-webpack-plugin": "^1.7.0",
    "jsdom": "^7.2.1",
    "mocha": "^2.3.4",
    "power-assert": "^1.2.0",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "history": "^1.17.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^1.0.3"
  }
}
