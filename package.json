{
  "private": true,
  "engines": {
    "node": ">=6"
  },
  "name": "react-starterkit",
  "version": "0.0.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf dist",
    "build:prod": "NODE_ENV=production webpack",
    "build": "yarn run clean && yarn build:prod",
    "start": "NODE_ENV=development babel-node server/index.js",
    "start:heroku": "node -r babel-register server/heroku.js",
    "lint": "eslint . --ext .js,.jsx",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "test": "yarn flow && yarn lint && yarn spec",
    "spec": "NODE_ENV=test NODE_PATH=src mocha --require test/helper.js test/**/*.spec.*",
    "spec:watch": "yarn spec -- -w",
    "heroku-postbuild": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zenoplex/react-starterkit.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform",
    "enzyme",
    "mocha"
  ],
  "author": "zenoplex (http://github.com/zenoplex)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zenoplex/react-starterkit/issues"
  },
  "homepage": "https://github.com/zenoplex/react-starterkit",
  "devDependencies": {
    "@webpack-blocks/babel6": "^0.4.1",
    "@webpack-blocks/extract-text2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-cli": "^6.3.17",
    "babel-eslint": "^7.2.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.24.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eventsource-polyfill": "^0.9.6",
    "flow-bin": "^0.42.0",
    "flow-typed": "^2.0.0",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "power-assert": "^1.2.0",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.4.3",
    "webpack": "^2.3.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-md5-hash": "^0.0.5"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.13.3",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "3",
    "recompose": "^0.22.0"
  }
}
